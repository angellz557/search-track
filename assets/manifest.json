{
  "manifest_version": 2,

  "name": "Tab Manager",
  "description": "Helps you track, manage, and learn from your web searches",
  "version": "0.1",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "notifications",
    "history",
    "webNavigation",
    "<all_urls>",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "page": "html/background.html",
    "persistent": true
  },  
  "web_accessible_resources": ["html/popup.html",
  "/vendor/bootstrap/dist/js/bootstrap.min.js",
  "/vendor/angular/angular.js",
  "/js/angular-ui-tree-master/dist/angular-ui-tree.min.js",
  "/vendor/taffydb/taffy-min.js",
  "/vendor/underscore/underscore-min.map",
  "/vendor/underscore/underscore-min.js",
  "/js/trackAPI.js.map",
  "/js/trackAPI.js",
  "/vendor/jquery/dist/jquery.min.map",
  "/vendor/jquery/dist/jquery.min.js",
  "/js/content/sidebar.html","/js/content/sidebar.js"],
  "icons": {
    "16": "img/structify_small.png",
    "48": "img/structify_medium.png",
    "128": "img/structify.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/vendor/jquery/dist/jquery.min.js",
      "/vendor/bootstrap/dist/js/bootstrap.min.js",
      "/vendor/angular/angular.js",
      "/js/angular-ui-tree-master/dist/angular-ui-tree.min.js",
      "/vendor/taffydb/taffy-min.js",
      "/vendor/underscore/underscore-min.js",
      "/js/trackAPI.js",
      "/js/hoverIntent.js",
      "/js/content/sidebar.js"],
      "run_at": "document_start"
    }
  ],
  "commands": {
    "add-importance-1": {
      "suggested_key": {
        "default": "Ctrl+Period",
        "mac": "Command+I"
      },
      "description": "1."
    },
    "add-importance-2": {
      "suggested_key": {
        "default": "Ctrl+Comma",
        "mac": "Command+K"
      },
      "description": "2."
    },
    "add-importance-3": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "Command+J"
      },
      "description": "3."
    },
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+Period"
      }
    }
  },
  "chrome_url_overrides": {
    "newtab": "html/visual.html"
  },
  "browser_action": {
    "default_icon": "img/structify_medium.png",
    "default_popup": "html/popup.html"
  }

}
